#include <iostream>
#include <cstdlib>
using namespace std;
void dig_trench(int n) {
int Trench [n][n];
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			Trench[i][j] = abs(i - j);
			cout << Trench[i][j]<<" ";
		}
		cout << endl;
	}
}
int main () {
int n;
cin>>n;
dig_trench(n);
return 0;
}
